{
  "name": "frontend",
  "version": "1.0.0",
  "scripts": {
    "start": "lite-server -c config-build.json",
    "start-dev": "tsc && concurrently \"npm run tsc:w\" \"npm run lite\" ",
    "lite": "lite-server -c config-dev.json",
    "build": "tsc && gulp",
    "postinstall": "typings install && jspm install",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "concurrently": "concurrently",
    "typings": "typings"
  },
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^2.2.0",
    "gulp": "^3.9.1",
    "gulp-html-replace": "^1.6.1",
    "gulp-htmlmin": "^2.0.0",
    "gulp-jspm": "^0.5.12",
    "gulp-manifest": "^0.1.1",
    "gulp-uglify": "^1.5.4",
    "jspm": "^0.16.39",
    "systemjs": "^0.19.31",
    "typescript": "^1.8.10",
    "typings": "^1.3.1"
  },
  "dependencies": {
    "lite-server": "^2.2.2"
  },
  "jspm": {
    "directories": {
      "baseURL": "src"
    },
    "dependencies": {
      "angular": "github:angular/bower-angular@^1.5.7",
      "angular-material": "github:angular/bower-material@^1.0.9"
    },
    "devDependencies": {}
  }
}
